<!--
	Generic table, complete with caption, header, footer, & body

	@param string %CAPTION%
	@param string %HEADERS% (both <thead> & <tfoot>)
	@param string %BODY%
-->
<table>
	<caption>%CAPTION%</caption>
	<thead>
		<tr>
			%HEADERS%
		</tr>
	</thead>
	<tfoot>
		<tr>
			%HEADERS%
		</tr>
	</tfoot>
	<tbody>
		%BODY%
	</tbody>
</table>
