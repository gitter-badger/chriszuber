<!--
	Template for generating RSS (feeds.rss)

	@param string %TITLE%
	@param string %URL%
	@param string %DESCRIPTION%
	@param string %CREATED%
-->
<item>
	<title>%TITLE%</title>
	<link>%URL%</link>
	<description>%DESCRIPTION%</description>
	<pubDate>%CREATED%</pubDate>
	<guid>%URL%</guid>
</item>
