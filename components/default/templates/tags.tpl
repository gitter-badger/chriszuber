<!--
	Template for sidebar keywords/tags

	@param string %TITLE%
	@param string %URL%
	@param string %DESCRIPTION%
	@param string %AUTHOR%
	@param string %DATE%
-->
<details class="tag">
	<summary><b>%TITLE%</b></summary>
	<a href="%URL%">
		<p>%DESCRIPTION%</p>
	</a>
	<p>
		By <a href="https://plus.google.com/%AUTHOR_URL%">%AUTHOR%</a> on %DATE%
	</p>
	<hr />
</details><br />
