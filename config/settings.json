{
    "time_zone": "America\/Los_Angeles",
    "define": {
        "theme": "default"
    },
    "requires": [
        "custom.php"
    ],
    "module_test": false,
    "apache_modules": [
        "mod_headers",
        "mod_mime"
    ],
    "php_modules": [
        "Core",
        "SPL",
        "curl",
        "date",
        "exif",
        "xml",
        "dom",
        "json",
        "PDO",
        "date",
        "pdo_mysql"
    ],
    "csp": {
        "default-src": "'self'",
        "base-uri": "'self'",
        "script-src": [
            "'self'",
            "*.google-analytics.com",
            "https:\/\/apis.google.com",
            "https:\/\/platform.twitter.com",
            "https:\/\/gist.github.com"
        ],
        "style-src": [
            "'self'",
            "https:\/\/gist-assets.github.com",
            "fonts.googleapis.com"
        ],
        "font-src": [
            "'self'",
            "themes.googleusercontent.com",
            "fonts.gstatic.com",
            "http://openfontlibrary.org"
        ],
        "img-src": [
            "'self'",
            "data:",
            "*.google-analytics.com",
            "*.googleusercontent.com",
            "chriszuber.com",
            "https://travis-ci.org",
            "https://api.travis-ci.org",
            "https://i.creativecommons.org",
            "https://licensebuttons.net"
        ],
        "media-src": [
            "'self'",
            "mediastream"
        ],
        "connect-src": "'self'",
        "frame-src": [
            "'self'",
            "*.youtube.com",
            "*.youtube-nocookie.com",
            "https:\/\/plusone.google.com",
            "https:\/\/facebook.com",
            "https:\/\/platform.twitter.com"
        ],
        "child-src": [
            "'self'",
            "*.youtube.com",
            "*.youtube-nocookie.com",
            "https:\/\/plusone.google.com",
            "https:\/\/facebook.com",
            "https:\/\/platform.twitter.com"
        ],
        "frame-ancestors": "'self'",
        "object-src": "'self'",
        "form-action": "'self'",
        "reflected-xss": "block",
        "plugin-types": "",
        "enforce": true
    }
}
